an> &gt;=3D <span class=3D"m">0</span> &amp;&amp; <span class=3D"nv">row</s=
pan> &lt; <span class=3D"nv">rows</span> &amp;&amp; <span class=3D"nv">colu=
mn</span> &gt;=3D <span class=3D"m">0</span> &amp;&amp; <span class=3D"nv">=
column</span> &lt; <span class=3D"nv">columns</span>
</li><li>    }
</li><li>    <span class=3D"k">subscript</span>(<span class=3D"nv">row</spa=
n>: <span class=3D"nc">Int</span>, <span class=3D"nv">column</span>: <span =
class=3D"nc">Int</span>) -&gt; <span class=3D"nc">Double</span> {
</li><li>        <span class=3D"k">get</span> {
</li><li>            <span class=3D"nv">assert</span>(<span class=3D"nv">in=
