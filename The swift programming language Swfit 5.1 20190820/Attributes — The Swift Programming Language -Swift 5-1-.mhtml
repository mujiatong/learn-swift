class=3D"Swift highlight"><ol class=3D"code-lines"><li><span class=3D"k">@a=
vailable</span>(<span class=3D"nv">swift</span> <span class=3D"m">3.0</span=
>.<span class=3D"m">2</span>)
</li><li><span class=3D"k">@available</span>(<span class=3D"k">macOS</span>=
 <span class=3D"m">10.12</span>, *)
</li><li><span class=3D"k">struct</span> <span class=3D"nv">MyStruct</span>=
 {
</li><li>    <span class=3D"c">// struct definition</span>
</li><li>}
</li></ol></div></div></div>
