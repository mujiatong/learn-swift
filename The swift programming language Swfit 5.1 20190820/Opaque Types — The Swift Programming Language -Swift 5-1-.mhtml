pan> =3D <span class=3D"nv">makeOpaqueContainer</span>(<span class=3D"nv">i=
tem</span>: <span class=3D"m">12</span>)
</li><li><span class=3D"k">let</span> <span class=3D"nv">twelve</span> =3D =
<span class=3D"nv">opaqueContainer</span>[<span class=3D"m">0</span>]
</li><li><span class=3D"nv">print</span>(<span class=3D"nv">type</span>(<sp=
an class=3D"nv">of</span>: <span class=3D"nv">twelve</span>))
</li><li><span class=3D"c">// Prints "Int"</span>
